<% if logged_in? %>
  <script type="text/javascript">
    window.currentUser = <%= render(
      "api/users/user.json.jbuilder",
      user: current_user
    ).html_safe %>
  </script>
<% end %>

<script type="text/javascript">
  window.searchIcon = "<%= image_url('search-icon.png') %>"
  window.githubLogo = "<%= image_url('github-logo2.png') %>"
  window.geniusLogo = "<%= image_url('genius-logo.png') %>"
  window.geniusNews = "<%= image_url('genius-news.png') %>"

</script>

<main id="root"></main>
